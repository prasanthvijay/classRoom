<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TblUser" table="tbl_User">
    <id name="userid" type="integer" column="userid">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="Username" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="password" type="string" column="password" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="surename" type="string" column="sureName" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="name" type="string" column="Name" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emailid" type="string" column="EmailId" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="mobileno" type="bigint" column="mobileNo" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="address" type="text" column="Address" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="gender" type="string" column="gender" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="employeetype" type="string" column="employeeType" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="department" type="integer" column="department" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="location" type="integer" column="location" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="chorusid" type="string" column="chorusId" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="gid" type="string" column="Gid" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="filename" type="string" column="filename" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="imagepath" type="string" column="imagePath" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usertypeid" type="integer" column="usertypeId" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="customerid" type="integer" column="customerId" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="adminid" type="integer" column="adminId" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="createdat" type="datetime" column="createdAt" nullable="true"/>
    <field name="status" type="string" column="status" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default">active</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
