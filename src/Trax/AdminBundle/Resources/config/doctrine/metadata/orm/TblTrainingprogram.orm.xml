<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TblTrainingprogram" table="tbl_trainingProgram">
    <id name="prgid" type="integer" column="prgId">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="batchid" type="integer" column="batchId" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="programname" type="string" column="programName" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="adminid" type="integer" column="adminId" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="createdat" type="datetime" column="createdAt" nullable="true"/>
  </entity>
</doctrine-mapping>
